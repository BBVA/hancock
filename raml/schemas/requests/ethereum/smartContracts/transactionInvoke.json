{
  "$schema": "http://json-schema.org/schema",
  "description": "Transactions schema",
  "type": "object",
  "properties": {
    "method": {
      "type": "string"
    },
    "action": {
      "type": "string",
      "enum": ["send", "call"]
    },
    "params": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "urlBase": {
      "type": "string"
    },
    "from": {
      "type": "string"
    },
    "to": {
      "type": "string"
    }
  },
  "required": [ "method", "urlBase", "from", "to" ]
}

