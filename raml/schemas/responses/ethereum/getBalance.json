{
    "$schema": "http://json-schema.org/schema",
    "description": "GetBalance Response Schema",
    "type": "object",
    "properties": {
      "result": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string"
          },
          "description": {
            "type": "string"
          }
        },
        "required": [ "code", "description" ]
      },
      "data": {
        "type": "object",
        "properties": {
          "balance": {
            "type": "string",
            "description": "Balance in wei"
          }
        }
      }
    },
    "required": [ "result", "data" ]
  }
  