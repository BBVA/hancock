{
  "$schema": "http://json-schema.org/schema",
  "description": "Register Token Response Schema",
  "type": "object",
  "properties": {
    "result": {
      "type": "object",
      "properties": {
        "code": {
          "type": "string"
        },
        "description": {
          "type": "string"
        }
      },
      "required": [
        "code",
        "description"
      ]
    },
    "data": {
      "address": {
        "type": "string"
      },
      "alias": {
        "type": "string"
      },
      "required": [
        "address",
        "alias"
      ]
    }
  }
}