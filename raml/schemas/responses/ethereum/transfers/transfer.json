{
    "$schema": "http://json-schema.org/schema",
    "description": "Transfer response schema",
    "type": "object",
    "properties": {
      "result": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string"
          },
          "description": {
            "type": "string"
          }
        },
        "required": [ "code", "description" ]
      },
      "data": {
        "type": "object",
        "properties": {
          "nonce": {
            "type": "string"
          },
          "from": {
            "type": "string"
          },
          "gas": {
            "type": "string"
          },
          "gasPrice": {
            "type": "string"
          },
          "to": {
            "type": "string"
          },
          "value": {
            "type": "string"
          },
          "data": {
            "type": "string"
          }
        }
      }
    },
    "required": [ "result", "data" ]
  }
  
  